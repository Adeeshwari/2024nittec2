#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int a,b,c;
    int total_revenue, total_cost,profit;
    scanf("%d %d %d", &a, &b, &c);
    total_revenue=a*b;
    total_cost = (a*c)+100;
    profit=total_revenue - total_cost;
    
    printf("%d\n",profit);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
